<ul class='posts'>

  <% posts.each(function (post) { %>
    <li>
      <%= post.escape('author_id') %>
      <div class="posts-index-item ">
        <img src="<%= post.get('image_url') %>" alt="" />
        <span class="body-username"><%= post.escape('author_id') %></span>
        <%= post.escape('body') %>
      </div>
    </li>
  <% }); %>

</ul>
